{
  "branches": [
    { "name": "dev" },
    { "name": "build-publish" },
    "build-publish-2"
  ],
  "repositoryUrl": "https://github.com/asu/asu-unity-stack.git",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    [
      "@semantic-release/github",
      {
        "successComment": false,
        "failComment": false,
        "assets": [
          {
            "path": [
              "dist/**/*.{js,css}",
              "build/**/*.{js,css}"
            ]
          }
        ]
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "CHANGELOG.md",
          "package.json"
        ]
      }
    ]
  ]
}
