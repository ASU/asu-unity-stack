    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "nodemon": "^1.17.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "style-loader": "^0.20.3",
