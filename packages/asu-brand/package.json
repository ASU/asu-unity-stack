{
  "name": "asu-brand",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "UNLICENSED",
  "private": false,
  "publishConfig": {
    "access": "restricted"
  },
  "repository" : {
    "type" : "git",
    "url": "ssh://git@github.com/asu/asu-unity-stack.git",
    "directory": "packages/asu-brand"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@testing-library/react": "^8.0.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.6.1",
    "css-loader": "^3.2.0",
    "eslint": "^5.3.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-react": "^7.14.2",
    "express": "^4.17.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "jest-cli": "^25.3.0",
    "mini-css-extract-plugin": "^0.4.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "regenerator-runtime": "^0.13.1",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "scripts": {
    "prepare": "yarn run build",
    "build": "webpack --config webpack/webpack.production.js && yarn prerender",
    "build:watch": "webpack --watch --config=webpack/webpack.development.js",
    "build:dev": "webpack --config=webpack/webpack.development.js",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -c .storybook -o export",
    "test": "jest",
    "lint": "eslint ./src --ext .js --ext .jsx",
    "prerender": "node scripts/prerender-templates.js"
  },
  "jest": {
    "verbose": true,
    "automock": false,
    "testURL": "http://localhost/",
    "setupFilesAfterEnv": [
      "@testing-library/react/cleanup-after-each"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "roots": [
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "globals": {
      "__DEV__": true
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "no-undef": "warn",
      "no-useless-escape": "off"
    },
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "version": "16.9"
      },
      "propWrapperFunctions": [
        "forbidExtraProps"
      ]
    }
  },
  "description": "ASU Brand React Components",
  "author": "Colton Testamarck (colt@asu.edu)",
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "gitHead": "7ac8ef038f8d07c1dad2cb858db4914e178b42fb"
}
