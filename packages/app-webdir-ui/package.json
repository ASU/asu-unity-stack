{
  "name": "@asu/app-webdir-ui",
  "version": "3.5.0",
  "description": "App Webdir UI",
  "main": "./dist/webdirUI.cjs.js",
  "browser": "./dist/webdirUI.umd.js",
  "module": "./dist/webdirUI.es.js",
  "types": "./dist/main.d.ts",
  "license": "MIT",
  "private": false,
  "files": [
    "examples/*",
    "dist/*",
    "docs/*",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/asu/asu-unity-stack.git",
    "directory": "packages/app-webdir-ui"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "lint": "eslint --fix 'src/**/*.{js,jsx}' --ignore-path ../../.eslintignore",
    "test": "jest --config=./jest.config.js --passWithNoTests --silent --coverage",
    "test-update-snapshot": "yarn test -- -u",
    "prebuild": "rm -rf ./dist",
    "build": "webpack -c webpack/webpack.prod.js",
    "build:stats": "webpack -c webpack/webpack.prod.js --profile --json=compilation-stats.json",
    "start:dev": "webpack-dashboard -- webpack serve -c webpack/webpack.dev.js",
    "storybook": "storybook dev -p 9030",
    "build-storybook": "storybook build -o ../../build/$npm_package_name",
    "jsdoc": "jsdoc -c jsdoc.config.js",
    "predocs": "mkdir -p ./docs",
    "docs": "jsdoc2md -c jsdoc.config.js --files ./src/** > ./docs/README.props.md",
    "postdocs": "node ../../scripts/process-readme-props.js"
  },
  "dependencies": {
    "@asu/components-core": "^3.0.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "axios": "^1.6.0",
    "prop-types": "^15.7.2",
    "react": ">= 16.14.0 < 18",
    "react-dom": ">= 16.14.0 < 18",
    "react-router-dom": "^6.2.1",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/runtime": "^7.15.3",
    "@babel/runtime-corejs3": "^7.15.3",
    "@storybook/addon-a11y": "^7.6.14",
    "@storybook/addon-essentials": "^7.6.14",
    "@storybook/addon-links": "^7.6.14",
    "@storybook/react": "^7.6.14",
    "@storybook/react-webpack5": "^7.6.14",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.4",
    "dotenv-webpack": "^7.0.3",
    "eslint-plugin-storybook": "^0.6.15",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "http-proxy-middleware": "^2.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-image-snapshot": "^4.4.1",
    "jest-transform-css": "^6.0.1",
    "jest-transform-file": "^1.1.1",
    "jsdoc": "^3.6.7",
    "jsdoc-plugin-typescript": "^2.0.6",
    "jsdoc-to-markdown": "5.0.0",
    "jsdoc-ts-utils": "^2.0.1",
    "jsdom-screenshot": "^4.0.0",
    "postcss-loader": "^6.1.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.39.2",
    "sass-loader": "^10",
    "semantic-release": "^17.4.2",
    "semantic-release-monorepo": "^7.0.4",
    "storybook": "^7.6.14",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "peerDependencies": {},
  "volta": {
    "extends": "../../package.json"
  }
}
