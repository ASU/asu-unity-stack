{
  "name": "@asu/unity-react-core",
  "version": "0.0.1",
  "main": "./dist/unity-react-core.umd.js",
  "module": "./dist/unity-react-core.mjs",
  "browser": "./dist/unity-react-core.umd.cjs",
  "types": "./dist/public/types/main.d.ts",
  "description": "Core UDS React UI components required by other higher-order React packages",
  "author": "Nathan Rollins <nathan.rollins@asu.edu>",
  "homepage": "https://github.com/ASU/asu-unity-stack#readme",
  "license": "MIT",
  "files": [
    "examples/*",
    "dist/*",
    "docs/*",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ASU/asu-unity-stack.git",
    "directory": "packages/unity-react-core"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/@asu"
  },
  "bugs": {
    "url": "https://github.com/ASU/asu-unity-stack/issues"
  },
  "scripts": {
    "create": "plop",
    "dev": "yarn storybook & yarn testui",
    "lint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}' --ignore-path ../../.eslintignore",
    "jest": "jest --config=./jest.config.js --silent --coverage",
    "test": "vitest --watch=false",
    "testui": "vitest --ui",
    "start:dev": "webpack-dashboard -- webpack serve -c webpack/webpack.dev.js",
    "build": "vite build -c vite.config.js && yarn loop && yarn postbuild && tsc",
    "build-individual-esm-components": "NUMBER=$NUMBER vite build -c vite.config.esm.js",
    "loop": "node ./scripts/loopComponentBuild.js",
    "postbuild": "cp ./types/main.d.ts ./dist/main.d.ts",
    "build:stats": "webpack -c webpack/webpack.prod.js --profile --json=compilation-stats.json",
    "storybook": "storybook dev -p 9200",
    "build-storybook": "storybook build -o ../../build/$npm_package_name",
    "jsdoc": "jsdoc -c jsdoc.config.js",
    "predocs": "mkdir -p ./docs",
    "docs": "jsdoc2md --no-cache -c jsdoc.config.js --files ./src/components > ./docs/README.props.md",
    "postdocs": "node ../../scripts/process-readme-props.js",
    "test:browser": "vitest --workspace=vitest.workspace.ts"
  },
  "devDependencies": {
    "@asu/unity-bootstrap-theme": "^1.0.0",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.21.3",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@storybook/addon-essentials": "^7.6.14",
    "@storybook/addon-interactions": "^7.6.14",
    "@storybook/addon-links": "^7.6.14",
    "@storybook/builder-vite": "^7",
    "@storybook/react": "^7.6.14",
    "@storybook/react-vite": "^7.6.14",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react-test-renderer": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^2.1.2",
    "@vitest/ui": "^2.1.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "eslint": "^8.37.0",
    "eslint-plugin-storybook": "^0.6.15",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "identity-obj-proxy": "^3",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-image-snapshot": "^4.4.1",
    "jest-transform-css": "^6.0.1",
    "jest-transform-file": "^1.1.1",
    "jsdoc": "4",
    "jsdom": "^24.0.0",
    "jsdom-screenshot": "^4.0.0",
    "playwright": "^1.48.0",
    "plop": "2.7.6",
    "postcss": "^8.4.19",
    "react-share": "^4.4.0",
    "reactstrap": "^9",
    "semantic-release": "^17.4.2",
    "semantic-release-monorepo": "^7.0.4",
    "storybook": "^7.6.14",
    "storybook-css-modules-preset": "^1.1.1",
    "style-loader": "^3.3.1",
    "terser": "^5.31.3",
    "typescript": "5.6.2",
    "vite": "^5.3.5",
    "vite-plugin-static-copy": "^1.0.6",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1",
    "vitest-browser-react": "^0.0.1"
  },
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.19.0",
    "phone-fns": "^3.2.3",
    "prop-types": "^15.7.2",
    "styled-components": "^5.3.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": ">= 5.2.0 < 7"
  },
  "exports": {
    "./*": "./*",
    ".": {
      "import": "./dist/unityReactCore.es.js",
      "require": "./dist/unityReactCore.umd.js"
    }
  },
  "volta": {
    "extends": "../../package.json"
  }
}
