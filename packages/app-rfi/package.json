{
  "name": "@asu-design-system/app-rfi",
  "version": "1.3.1",
  "description": "ASU Request For Information (RFI) form",
  "author": "Michael Samuelson <mlsamuel@asu.edu>",
  "homepage": "https://github.com/ASU/asu-unity-stack#readme",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ASU/asu-unity-stack.git"
  },
  "scripts": {
    "lint": "eslint --fix 'src/**/*.{js,jsx}' --ignore-path ../../.eslintignore",
    "test": "jest --config=./jest.config.js",
    "start:dev": "webpack-dashboard -- webpack serve -c webpack/webpack.dev.js",
    "build": "webpack -c webpack/webpack.prod.js",
    "build:stats": "webpack -c webpack/webpack.prod.js --profile --json=compilation-stats.json",
    "storybook": "start-storybook -p 6006",
    "percy": "build-storybook -c ./.storybook/percy -o ./percy-storybook && export PERCY_TOKEN=$PERCY_TOKEN_APP_RFI && percy storybook ./percy-storybook",
    "build-storybook": "build-storybook",
    "jsdoc": "jsdoc -c jsdoc.config.js",
    "predocs": "mkdir -p ./docs",
    "docs": "jsdoc2md --no-cache -c jsdoc.config.js --files ./src/components > ./docs/README.props.md",
    "postdocs": "node ../../scripts/process-readme-props.js"
  },
  "bugs": {
    "url": "https://github.com/ASU/asu-unity-stack/issues"
  },
  "devDependencies": {
    "@percy/cli": "^1.0.0-beta.68",
    "@percy/storybook": "^4.0.3",
    "@asu-design-system/bootstrap4-theme": "latest",
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@storybook/addon-actions": "^6.2.3",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.3",
    "@storybook/addon-links": "^6.2.3",
    "@storybook/react": "^6.2.3",
    "@testing-library/react": "^11.2.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "eslint": "^7.25.0",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.4.1",
    "jest-transform-css": "^2.1.0",
    "jest-transform-file": "^1.1.1",
    "jsdoc": "^3.6.7",
    "jsdoc-plugin-typescript": "^2.0.6",
    "jsdoc-to-markdown": "5.0.0",
    "jsdoc-ts-utils": "^2.0.1",
    "jsdom-screenshot": "^4.0.0",
    "postcss": "^7.0.2",
    "semantic-release": "^17.4.2",
    "semantic-release-monorepo": "^7.0.4",
    "storybook-css-modules-preset": "^1.0.7",
    "style-loader": "^2.0.0"
  },
  "dependencies": {
    "@asu-design-system/components-core": "^1.0.0",
    "formik": "^2.1.4",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-phone-input-2": "^2.14.0",
    "reactstrap": "^8.9.0",
    "styled-components": "^5.0.1",
    "yup": "^0.28.1",
    "yup-phone": "^1.2.19"
  }
}
