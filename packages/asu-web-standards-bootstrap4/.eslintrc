{
  "parserOptions": {
    "ecmaVersion": 3,
    "sourceType": "script"
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {},
  "rules": {
    // "semi": [
    //   2,
    //   "never"
    // ],
    "eqeqeq": 0,
    "no-eq-null": 2,
    // "no-unused-expressions": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "linebreak-style": 2,
    "no-irregular-whitespace": 2,
    "strict": [
      2,
      "function"
    ],
    // "no-undef": 2,
    "no-unused-vars": 2,
    "no-empty": [
      2,
      {
        "allowEmptyCatch": true
      }
    ],
    "no-with": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-str": 2,
    "one-var": [
      2,
      "never"
    ],
    "quote-props": [
      2,
      "as-needed",
      {
        "keywords": true
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "space-before-function-paren": [
      2,
      "always"
    ],
    "array-bracket-spacing": [
      2,
      "always",
      {
        "objectsInArrays": false
      }
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "no-trailing-spaces": 2,
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "dot-notation": 2,
    "eol-last": 2,
    "space-infix-ops": 2,
    "keyword-spacing": [
      2,
      {}
    ],
    "spaced-comment": [
      2,
      "always"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ]
  }
}
