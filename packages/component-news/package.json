{
  "name": "@asu/component-news",
  "version": "1.1.2",
  "description": "ASU News component",
  "main": "./dist/asuNews.cjs.js",
  "browser": "./dist/asuNews.umd.js",
  "module": "./dist/asuNews.es.js",
  "types": "./dist/main.d.ts",
  "author": "Michele Dibenedetto",
  "license": "MIT",
  "private": false,
  "files": [
    "examples/*",
    "dist/*",
    "docs/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/asu/asu-unity-stack.git",
    "directory": "packages/component-news"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "lint": "eslint --fix 'src/**/*.{js,jsx}' --ignore-path ../../.eslintignore",
    "test": "jest --config=./jest.config.js --passWithNoTests --silent --coverage",
    "prebuild": "rm -rf ./dist",
    "test-update-snapshot": "yarn test -- -u",
    "build": "webpack -c webpack/webpack.prod.js",
    "build:stats": "webpack -c webpack/webpack.prod.js --profile --json=compilation-stats.json",
    "postbuild": "cp ./types/main.d.ts ./dist/main.d.ts",
    "start:dev": "webpack-dashboard -- webpack serve -c webpack/webpack.dev.js",
    "storybook": "start-storybook -s ./dist -p 9011",
    "build-storybook": "build-storybook -s ./dist",
    "jsdoc": "jsdoc -c jsdoc.config.js",
    "predocs": "mkdir -p ./docs",
    "docs": "jsdoc2md --no-cache -c jsdoc.config.js --files ./src/components > ./docs/README.props.md",
    "postdocs": "node ../../scripts/process-readme-props.js"
  },
  "dependencies": {
    "@asu/component-carousel": "^1.0.0",
    "@asu/components-core": "^1.0.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "prop-types": "^15.7.2",
    "react": ">= 16.14.0 < 18",
    "react-dom": ">= 16.14.0 < 18",
    "storybook-addon-mock": "1.7.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/runtime": "^7.15.3",
    "@babel/runtime-corejs3": "^7.15.3",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-controls": "^6.5.15",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.4",
    "dotenv-webpack": "^7.0.3",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "http-proxy-middleware": "^2.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-image-snapshot": "^4.4.1",
    "jest-transform-css": "^2.1.0",
    "jest-transform-file": "^1.1.1",
    "jsdoc": "^3.6.7",
    "jsdoc-plugin-typescript": "^2.0.6",
    "jsdoc-to-markdown": "5.0.0",
    "jsdoc-ts-utils": "^2.0.1",
    "jsdom-screenshot": "^4.0.0",
    "postcss-loader": "^6.1.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.39.2",
    "sass-loader": "^10",
    "semantic-release": "^17.4.2",
    "semantic-release-monorepo": "^7.0.4",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-merge": "^5.8.0"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "1.22.10"
  }
}
