{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "prebuild",
          "build",
          "build-storybook"
        ]
      }
    }
  },
  "targetDefaults": {
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "prebuild": {
      "dependsOn": [
        "^prebuild"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build-storybook": {
      "dependsOn": [
        "build",
        "build-storybook"
      ]
    }
  }
}
